pipeline {
   agent any
   tools {
      maven 'maven'
    }
    stages{
        stage('Clean workspace') {
             steps {
                echo 'Cleaning Workspace...'
                deleteDir()
            }
        }
        stage(checkout) {
            steps {
                git url: 'https://github.com/gayathri-panneerselvam/mvn-test.git'
            }
        }
        stage(build){
            steps {
                sh 'mvn install'
                  }
        }

    }
}
